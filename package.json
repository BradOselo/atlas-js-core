{
  "name": "atlas-sdk-core",
  "version": "4.0.0",
  "description": "Atlas JavaScript SDK Core for Zalando Checkout, Guest Checkout, and Catalog APIs.",
  "main": "lib/index.node.js",
  "browser": "lib/index.js",
  "files": [
    "src/",
    "lib/"
  ],
  "keywords": [
    "atlas",
    "guest checkout",
    "checkout",
    "catalog",
    "api",
    "extensions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zalando-incubator/atlas-js-core.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src test",
    "clean": "rm -rf lib",
    "dev:build": "npm run clean && ./node_modules/.bin/webpack --config webpack.config.dev.js --watch",
    "production:build": "npm run clean && ./node_modules/.bin/webpack --config webpack.config.js",
    "david": "./node_modules/.bin/david",
    "check": "nsp check package",
    "cover": "nyc --reporter lcov --reporter text npm run test",
    "cover:check": "nyc check-coverage",
    "cover:report-coverage": "npm run cover && cat ./coverage/lcov.info | codecov",
    "test": "FORCE_COLOR=t ./node_modules/.bin/ava --verbose | tap-notify | tap-diff",
    "tdd": "ava --watch",
    "validate": "npm run lint && npm test",
    "prepare": "npm run validate && npm run production:build && npm run check",
    "release:production": "npm run prepare && npm run docs",
    "docs": "rm -rf docs && ./node_modules/.bin/jsdoc -r -c ./jsdoc_conf.json -t ./node_modules/ink-docstrap/template -d docs/ -R README.md",
    "docs:deploy": "./node_modules/.bin/gh-pages -d docs",
    "gh-pages-travis": "gh-pages-travis",
    "commit": "./node_modules/.bin/git-cz",
    "prepush": "npm run validate",
    "commitmsg": "validate-commit-msg" ,
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Zalando SE",
  "maintainers": [
    {
      "name": "Ahmed Shehata",
      "email": "ahmed.shehata@zalando.de",
      "web": "https://github.com/ahmed-shehata"
    },
    {
      "name": "Gleb Galkin",
      "email": "gleb.galkin@zalando.de",
      "web": "https://github.com/artdaw"
    }
  ],
  "dependencies": {
    "isomorphic-fetch": "2.2.1"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "0.17.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "1.1.3",
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "david": "11.0.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "fetch-mock": "^5.11.0",
    "gh-pages": "^1.0.0",
    "gh-pages-travis": "^1.0.4",
    "husky": "^0.14.2",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "json-loader": "^0.5.4",
    "node-noop": "^1.0.0",
    "nsp": "^2.2.0",
    "nyc": "^10.3.2",
    "semantic-release": "^6.3.5",
    "tap-diff": "0.1.1",
    "tap-notify": "1.0.0",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^2.6.0",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": true,
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 200,
      "subjectPattern": ".+",
      "helpMessage": "Please, check your commit message format.",
      "autoFix": true
    }
  },
  "ava": {
    "babel": {
      "presets": [
        "@ava/stage-4"
      ]
    },
    "require": "babel-register",
    "concurrency": 2,
    "tap": true
  }
}